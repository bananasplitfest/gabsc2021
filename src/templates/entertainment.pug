extends partials/layout

//- Override default config variables
append config
    - var vendorStyles = ['tiny-slider/dist/tiny-slider.css', 'lightgallery.js/dist/css/lightgallery.min.css']
    - var vendorScripts = ['tiny-slider/dist/min/tiny-slider.js', 'lightgallery.js/dist/js/lightgallery.min.js', 'lg-video.js/dist/lg-video.min.js']
    - var parentMenu = 'Entertainment'
    - var pageTitle = 'Entertainment'

//- Navbar
block navbar
    include partials/navbar-solid-shadow

//- Page Content
block content
  -
    activities.sort( function(a,b) {
      return a.highlight ? -1 : 0 || new Date(a.start) - new Date(b.start) || new Date(a.end) - new Date(b.end) || a.title.localeCompare(b.title);   
    });
    let resourcesArray = {}
    for (const i of resources) {
      resourcesArray[i.id] = i
    }
  

  .container.py-4.mb-2.mb-sm-0.pb-sm-5
    .row.justify-content-center
      .col-lg-9
        h1.mb-2 Entertainment
        p.fs-4.mb-5 The line-up for the 2022 Great American Banana Split Celebration is here.

        .entertainment
          each a in activities
            if a.start && a.entertainer
              // Band
              article.card.card-horizontal.mb-grid-gutter&attributes(a.highlight ? {class: "bandShadow border-5", style: "border-color: #0757a4;"} : "")
                //- , a.image ? "" : {class: "col-md-6 col-lg-4 col-xl-3"}
                if a.image && a.image.page
                  img(src='img/entertainment/2022/'+a.image.page.file).card-img-top
                .card-body
                  if a.pre
                    .fs-sm.m-0 #{a.pre}
                  h2.h3.nav-heading.m-0
                    | #{a.title}
                  if a.post
                    .fs-sm.m-0 #{a.post}
                  
                  if a.start || a.resourceId || a.genre
                    ul.list-unstyled.fs-sm.align-items-center.m-0.mt-3
                      if a.genre
                        li.list-inline-item.m-0.me-3
                          i.fal.fa-music.fa-fw
                          | &nbsp; #{a.genre}
                      if a.start
                        li.list-inline-item.m-0.me-3
                          i.fal.fa-calendar-day.fa-fw
                          | &nbsp; #{ daysAbbv[new Date(a.start).getDay()] }, #{ monthsAbbv[new Date(a.start).getMonth()] } #{ new Date(a.start).getDate() }
                      if a.start
                        li.list-inline-item.m-0.me-3
                          i.fal.fa-clock.fa-fw
                          | &nbsp; #{new Date(a.start).toLocaleTimeString('en-US',{ timeStyle: 'short' }) }
                      if a.resourceId
                        li.list-inline-item.m-0.me-3
                          i.fal.fa-map-marker-alt.fa-fw
                          | &nbsp; #{resourcesArray[a.resourceId].title}
                      

                  if a.description
                    .d-flex.fs-sm.align-items-center.mt-3
                      | #{a.description}

                  if a.links
                    ul.list-unstyled.m-0.align-items-center.mt-4
                      each l in a.links
                        case l.type
                          when "website"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.far.fa-link.fa-fw
                                | &nbsp;Website
                          when "facebook"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-facebook.fa-fw
                                | &nbsp;Facebook
                          when "instagram"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-instagram.fa-fw
                                | &nbsp;Instagram
                          when "twitter"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-twitter.fa-fw
                                | &nbsp;Twitter
                          when "spotify"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-spotify.fa-fw
                                | &nbsp;Spotify
                          when "appleMusic"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-apple.fa-fw
                                | &nbsp;Apple Music
                          when "youtube"
                            li.list-inline-item.m-0.me-2
                              a(href=l.link, target="_blank").meta-link.fs-xs
                                i.me-1.fab.fa-youtube.fa-fw
                                | &nbsp;YouTube
        
