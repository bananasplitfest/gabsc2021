extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['lightgallery.js/dist/css/lightgallery.min.css']
  - var vendorScripts = ['jarallax/dist/jarallax.min.js', 'jarallax/dist/jarallax-element.min.js', 'lightgallery.js/dist/js/lightgallery.min.js', 'lg-fullscreen.js/dist/lg-fullscreen.min.js', 'lg-zoom.js/dist/lg-zoom.min.js', 'lg-video.js/dist/lg-video.min.js']
  - var parentMenu = 'Index'
  - var pageTitle = 'Home'

//- Navbar
