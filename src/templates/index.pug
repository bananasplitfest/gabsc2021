  section.bg-secondary.py-5.py-md-6
    .container.py-2.py-md-0
      .row.align-items-center
        .col-xl-4.text-center.text-xl-start
          h2.mb-4.mb-xl-0 Be at the celebration
        .col-xl-8
          .countdown.h2.display-2.justify-content-center.justify-content-xl-start(data-countdown='08/21/2021')
            .countdown-days.mb-0.mt-3.me-0.px-4.border-end
              span.countdown-value.fw-normal.px-4 0
              span.countdown-label.fs-lg.text-body Days
            .countdown-hours.mb-0.mt-3.me-0.px-4.border-end
              span.countdown-value.fw-normal.px-4 0
              span.countdown-label.fs-lg.text-body Hours
            .countdown-minutes.mb-0.mt-3.me-0.px-4.border-end
              span.countdown-value.fw-normal.px-4 0
              span.countdown-label.fs-lg.text-body Mins
            .countdown-seconds.mb-0.mt-3.me-0.px-4
              span.countdown-value.fw-normal.px-4 0
              span.countdown-label.fs-lg.text-body Secs
  

